    "import cv2\n",
    "import numpy as np\n",
    "\n",
    "path = r'geeks14.png'\n",
    "\n",
    "image = cv2.imread(path)\n",
    "\n",
    "window_name = 'Image'\n",
    "\n",
    "kernel = np.ones((5,5),np.uint8)\n",
    "\n",
    "image = cv2.erode(image, kernel)\n",
    "\n",
    "cv2.imshow(window_name, image)\n",
    "\n",
    "cv2.waitKey(0)\n"
   ]
  }
